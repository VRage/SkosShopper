<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.control.cell.PropertyValueFactory?>


<AnchorPane prefHeight="207.0" prefWidth="219.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="controller.TriplesShowController">
   <children>
      <Pane layoutY="-3.0" prefHeight="300.0" prefWidth="815.0">
         <children>
            <ScrollPane layoutY="49.0" prefHeight="252.0" prefWidth="815.0">
               <content>
                  <TableView fx:id="tableTriples" prefHeight="242.0" prefWidth="798.0">
                    <columns>
                    
                      <TableColumn fx:id="object" prefWidth="170.0" text="Object">
                      	<cellValueFactory><PropertyValueFactory property="object" /></cellValueFactory>
                      </TableColumn>
                      
                      <TableColumn fx:id="predicate" minWidth="3.0" prefWidth="174.0" text="Predicate">
                      	<cellValueFactory><PropertyValueFactory property="predicate" /></cellValueFactory>
                      </TableColumn>
                      
                      <TableColumn fx:id="subject" prefWidth="75.0" text="Subject">
                      	<cellValueFactory><PropertyValueFactory property="subject" /></cellValueFactory>
                      </TableColumn>
                      
                    </columns>
                  </TableView>
               </content>
            </ScrollPane>
            <Button fx:id="loadTriplesFromServerBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#loadTriplesFromServer" text="Load Triples from Server" />
            <Label fx:id="loadTriplesMsg" layoutX="179.0" layoutY="18.0" text="No Triples Loaded from the Server" />
         </children>
      </Pane>
   </children>
</AnchorPane>
